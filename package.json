      "eslint --fix --cache"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^5.81.5",
    "axios": "^1.10.0",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-onboarding": "^9.0.15",
    "@storybook/nextjs": "^9.0.15",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-plugin-storybook": "^9.0.15",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.2",
    "prettier": "^3.6.2",
    "storybook": "^9.0.15",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
